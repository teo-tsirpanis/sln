<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net45;netstandard2.0</TargetFrameworks>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <Description>A .NET solution file parser that can recognize standalone files inside solution folders.
If your project does not need this feature, use the official Microsoft.Build package instead.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <DefineConstants>FULL_SLN_PARSER;STANDALONEBUILD;$(DefineConstants)</DefineConstants>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <Copyright>Using modified source code of MSBuild, repackaged and modified by Enrico Sada.</Copyright>
    <PackageReleaseNotes>Change the namespace to EnricoSada.MSBuild to avoid name clashes with the MSBuild library.</PackageReleaseNotes>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0" PrivateAssets="all" />
  </ItemGroup>
</Project>
